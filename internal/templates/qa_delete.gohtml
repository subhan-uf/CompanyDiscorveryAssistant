{{define "content_qa_delete"}}
<section>
  <h2>Delete Q&A</h2>
  <p>Are you sure you want to delete this Q&A?</p>
  <blockquote>{{.Question}}</blockquote>
  <form method="post" action="/qa/delete?id={{.ID}}">
    <button type="submit" class="danger">Yes, delete</button>
    <a class="btn" href="/qa">Cancel</a>
  </form>
</section>
{{end}}
